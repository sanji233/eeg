    "def process_eeg_data(raw, events, epoch_duration=6.0):\n",
    "    \"\"\"\n",
    "    处理EEG数据，仅提取任务相关的epochs\n",
    "    \n",
    "    参数:\n",
    "        raw: MNE Raw对象\n",
    "        events: 事件数组\n",
    "        epoch_duration: 每个epoch的持续时间（秒）\n",
    "        \n",
    "    返回:\n",
    "        X_epochs: 形状为(n_epochs, n_channels, n_times)的numpy数组\n",
    "        y_epochs: 形状为(n_epochs,)的numpy数组，标签\n",
    "    \"\"\"\n",
    "    \n",
    "    # 定义标签映射\n",
    "    numeric_label_map = {\n",
    "        769: 0,  # 左\n",
    "        770: 1,  # 右\n",
    "        780: 2,  # 上\n",
    "        774: 3   # 下\n",
    "    }\n",
    "    \n",
    "    # 提取任务相关epochs（左、右、上、下）\n",
    "    task_event_id = {\n",
    "        'left': 769,\n",
    "        'right': 770,\n",
    "        'up': 780,\n",
    "        'down': 774\n",
    "    }\n",
    "    \n",
    "    # 创建epochs\n",
    "    task_epochs = mne.Epochs(\n",
    "        raw,\n",
    "        events=events,\n",
    "        event_id=task_event_id,\n",
    "        tmin=0.0,\n",
    "        tmax=epoch_duration,\n",
    "        baseline=None,\n",
    "        picks='eeg',\n",
    "        preload=True\n",
    "    )\n",
    "    \n",
    "    # 获取数据并转换单位（从μV到V）\n",
    "    X_epochs = (task_epochs.get_data() * 1e-6).astype(np.float32)\n",
    "    y_epochs = np.array([numeric_label_map[val] for val in task_epochs.events[:, 2]], dtype=np.int64)\n",
    "    \n",
    "    print(f\"任务相关epochs数量: {len(task_epochs)}\")\n",
    "    print(f\"数据形状: {X_epochs.shape}\")  # (n_epochs, n_channels, n_times)\n",
    "    print(f\"标签形状: {y_epochs.shape}\")  # (n_epochs,)\n",
    "    \n",
    "    return X_epochs, y_epochs"
   ]
  },
